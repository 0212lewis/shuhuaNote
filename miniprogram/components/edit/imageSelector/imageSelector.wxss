/* pages/edit/components/imageSelector/imageSelector.wxss */
.selector-wrapper {
    display: flex;
    height: 165rpx;
    margin-bottom: 20rpx;
}
.selected-item{
    float: left;
    position: relative;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    width: 165rpx;
    height: 165rpx;
}

.selected-item > image {
    width: 100%;
    height: 100%;
}

.selector {
    position: relative;
    width: 165rpx;
    height: 165rpx;
    background-color: #eeeeee;
    display: flex;
    align-items: center;
    justify-content: center;
}
